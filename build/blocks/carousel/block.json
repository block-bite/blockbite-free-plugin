{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "blockbite/carousel",
  "version": "0.1.0",
  "title": "Carousel",
  "category": "blockbite",
  "icon": "flag",
  "description": "A carousel slider block.",
  "providesContext": {
    "blockbite/swiperEditMode": "swiperEditMode"
  },
  "attributes": {
    "media": {
      "type": "object",
      "default": {
        "id": 0,
        "sizes": {},
        "url": "https://placehold.co/200x200/000000/FFFFFF/png",
        "alt": "",
        "type": "",
        "width": 0,
        "height": 0
      }
    },
    "biteId": {
      "type": "string",
      "default": ""
    },
    "biteImage": {
      "type": "array",
      "default": []
    },
    "imageStyle": {
      "type": "array",
      "default": []
    },
    "imageClass": {
      "type": "string",
      "default": ""
    },
    "flexStyle": {
      "type": "array",
      "default": []
    },
    "flexClass": {
      "type": "string",
      "default": ""
    },
    "anchor": {
      "type": "string",
      "default": ""
    },
    "metadata": {
      "type": "object",
      "default": {
        "name": "Carousel"
      }
    },
    "swiperEditMode": {
      "type": "string",
      "default": "play"
    },
    "carouselSettings": {
      "type": "object",
      "default": {
        "navigation": true,
        "slidesPerView": {
          "xs": 1,
          "md": 1,
          "lg": 1,
          "xl": 1
        },
        "speed": "500",
        "loop": false,
        "autoplay": {
          "delay": "0",
          "disableOnInteraction": true
        },
        "pagination": null,
        "spaceBetween": 0,
        "cssMode": false,
        "init": true,
        "freeMode": false,
        "newsticker": false
      }
    }
  },
  "supports": {
    "html": false,
    "anchor": true,
    "color": {
      "gradients": true,
      "link": true,
      "background": true,
      "text": true
    },
    "spacing": {
      "margin": true,
      "padding": true,
      "blockGap": true
    },
    "renaming": true,
    "align": [
      "left",
      "right",
      "wide",
      "full"
    ]
  },
  "textdomain": "blockbite",
  "editorScript": "file:./index.js"
}