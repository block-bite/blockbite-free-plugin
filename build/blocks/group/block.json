{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "blockbite/group",
  "version": "0.1.0",
  "title": "Group",
  "category": "blockbite",
  "icon": "flag",
  "description": "A advanced group block",
  "attributes": {
    "media": {
      "type": "object",
      "default": {
        "id": 0,
        "sizes": {},
        "url": "https://placehold.co/200x200/000000/FFFFFF/png",
        "alt": "",
        "type": "",
        "width": 0,
        "height": 0
      }
    },
    "mediaStyle": {
      "type": "array",
      "default": []
    },
    "mediaClass": {
      "type": "string",
      "default": ""
    },
    "flexStyle": {
      "type": "array",
      "default": []
    },
    "flexClass": {
      "type": "string",
      "default": ""
    },
    "linkUrl": {
      "type": "string",
      "default": ""
    },
    "linkTile": {
      "type": "boolean",
      "default": false
    },
    "linkTitle": {
      "type": "string",
      "default": "Tile"
    },
    "linkTarget": {
      "type": "string",
      "default": "_self"
    },
    "anchor": {
      "type": "string",
      "default": ""
    },
    "metadata": {
      "type": "object",
      "default": {
        "name": "Group"
      }
    }
  },
  "supports": {
    "html": false,
    "anchor": true,
    "color": {
      "gradients": true,
      "link": true,
      "background": true,
      "text": true
    },
    "dimensions": {
      "minHeight": true
    },
    "spacing": {
      "margin": true,
      "padding": true
    },
    "renaming": true,
    "align": [
      "left",
      "right",
      "wide",
      "full"
    ]
  },
  "textdomain": "blockbite",
  "editorScript": "file:./index.js"
}